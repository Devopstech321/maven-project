pipeline
{
    agent any
    stages
    {
        stage ('scm checkout')
        {
            steps {git branch: 'master', url: 'https://github.com/Devopstech321/maven-project.git'}
        }
        stage ('create artifact')
        {
           steps {withMaven(globalMavenSettingsConfig: jdk: 'JAVA-HOME', maven: 'Maven_Home', mavenSettingsConfig:) {
        sh 'mvn package'
}} 
        }
    }
}